<?xml version="1.0" encoding="UTF-8"?>
<ValidationPatterns>
	<ValidationPattern label="Credit Card Number">
		<Pattern><![CDATA[^((4\d{3})|(5[1-5]\d{2})|(6011)|(7\d{3}))-?\d{4}-?\d{4}-?\d{4}|3[4,7]\d{13}$]]></Pattern>
	    <Default><![CDATA[4444444444444444]]></Default>
	</ValidationPattern>

	<ValidationPattern label="URL">
		<Pattern><![CDATA[^((((https?|ftps?|gopher|telnet|nntp)://)|(mailto:|news:))(%[0-9A-Fa-f]{2}|[-()_.!~*';/?:@&=+$,A-Za-z0-9])+)([).!';/?:,][[:blank:]])?$]]></Pattern>
	    <Default><![CDATA[http://www.google.com]]></Default>
	</ValidationPattern>

	<ValidationPattern label="IP Address">
		<Pattern><![CDATA[^(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)$]]></Pattern>
	    <Default><![CDATA[209.85.135.103]]></Default>
	</ValidationPattern>

	<ValidationPattern label="Email">
		<Pattern><![CDATA[^[\w\-\+\&\*]+(?:\.[\w\-\_\+\&\*]+)*@(?:[\w-]+\.)+[a-zA-Z]{2,7}$]]></Pattern>
        <Default><![CDATA[admin@gmail.com]]></Default>
	</ValidationPattern>

	<ValidationPattern label="Safe_Text">
		<Pattern><![CDATA[^[a-zA-Z0-9\s.\-]+$]]></Pattern>
	    <Default><![CDATA[safe_text]]></Default>
	</ValidationPattern>

	<ValidationPattern label="Date">
		<Pattern><![CDATA[^(?:(?:(?:0?[13578]|1[02])(\/|-|\.)31)\1|(?:(?:0?[1,3-9]|1[0-2])(\/|-|\.)(?:29|30)\2))(?:(?:1[6-9]|[2-9]\d)?\d{2})$|^(?:0?2(\/|-|\.)29\3(?:(?:(?:1[6-9]|[2-9]\d)?(?:0[48]|[2468][048]|[13579][26])|(?:(?:16|[2468][048]|[3579][26])00))))$|^(?:(?:0?[1-9])|(?:1[0-2]))(\/|-|\.)(?:0?[1-9]|1\d|2[0-8])\4(?:(?:1[6-9]|[2-9]\d)?\d{2})$]]></Pattern>
        <Default><![CDATA[1900-01-01]]></Default>
	</ValidationPattern>

	<ValidationPattern label="Password">
		<Pattern><![CDATA[^(?=.*\d)(?=.*[a-z])(?=.*[A-Z]).{4,8}$]]></Pattern>
	    <Default><![CDATA[password]]></Default>
	</ValidationPattern>

	<ValidationPattern label="English_digitwords">
		<Pattern><![CDATA[^(zero|one|two|three|four|five|six|seven|eight|nine)$]]></Pattern>
        <Default><![CDATA[zero]]></Default>
	</ValidationPattern>

	<ValidationPattern label="English_daywords">
		<Pattern><![CDATA[^(Mo|Tu|We|Th|Fr|Sa|Su)$]]></Pattern>
	    <Default><![CDATA[Su]]></Default>
	</ValidationPattern>

	<ValidationPattern label="English_monthwords">
		<Pattern><![CDATA[^(Jan|Feb|Mar|Apr|May|Jun|Jul|Aug|Sep|Oct|Nov|Dec)$]]></Pattern>
	    <Default><![CDATA[Dec]]></Default>
	</ValidationPattern>

	<ValidationPattern label="French_digitwords">
		<Pattern><![CDATA[^(z[eé]ro|un|deux|trois|quatre|cing|six|sept|huit|neuf)$]]></Pattern>
        <Default><![CDATA[neuf]]></Default>
	</ValidationPattern>

	<ValidationPattern label="German_digitwords">
		<Pattern><![CDATA[^(null|eins|zwei|drei|vier|f(ue|ü)nf|sechs|sieben|acht|neun)$]]></Pattern>
        <Default><![CDATA[neun]]></Default>
	</ValidationPattern>


	<ValidationPattern label="Spanish_digitwords">
		<Pattern><![CDATA[^(cero|uno|dos|tres|cuatro|cinco|seis|siete|ocho|nueve)$]]></Pattern>
        <Default><![CDATA[nueve]]></Default>
	</ValidationPattern>

	<ValidationPattern label="US_ZIPCODE">
		<Pattern><![CDATA[^\d{5}(-\d{4})?$]]></Pattern>
	    <Default><![CDATA[20500]]></Default>
	</ValidationPattern>

	<ValidationPattern label="US_PHONE">
		<Pattern><![CDATA[^\D?(\d{3})\D?\D?(\d{3})\D?(\d{4})$]]></Pattern>
        <Default><![CDATA[(202)456-1111]]></Default>
	</ValidationPattern>

	<ValidationPattern label="US_STATE">
		<Pattern><![CDATA[^(AE|AL|AK|AP|AS|AZ|AR|CA|CO|CT|DE|DC|FM|FL|GA|GU|HI|ID|IL|IN|IA|KS|KY|LA|ME|MH|MD|MA|MI|MN|MS|MO|MP|MT|NE|NV|NH|NJ|NM|NY|NC|ND|OH|OK|OR|PW|PA|PR|RI|SC|SD|TN|TX|UT|VT|VI|VA|WA|WV|WI|WY)$]]></Pattern>
        <Default><![CDATA[NC]]></Default>
	</ValidationPattern>

	<ValidationPattern label="US_SSN">
		<Pattern><![CDATA[^\d{3}-\d{2}-\d{4}$]]></Pattern>
		<Default><![CDATA[123-45-6789]]></Default>
	</ValidationPattern>

</ValidationPatterns>
